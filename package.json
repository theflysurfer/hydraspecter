{
  "name": "concurrent-browser-mcp-stealth",
  "version": "2.0.0",
  "description": "Concurrent browser MCP with stealth mode, ARIA snapshots, batch execution & session persistence. Fork optimized for token efficiency.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "concurrent-browser-mcp-persistent": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:proxy": "node test-proxy.js",
    "benchmark": "node benchmark/run-benchmark.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "browser",
    "automation",
    "playwright",
    "concurrent",
    "stealth",
    "aria-snapshot",
    "batch-execute",
    "token-optimization"
  ],
  "author": "sailaoda",
  "license": "Apache-2.0",
  "homepage": "https://github.com/sailaoda/concurrent-browser-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sailaoda/concurrent-browser-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/sailaoda/concurrent-browser-mcp/issues"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "config.example.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "playwright": "npm:rebrowser-playwright@^1.52.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "playwright": "npm:rebrowser-playwright@^1.52.0"
  }
}
