{
  "mcpServers": {
    "hydraspecter": {
      "command": "npx",
      "args": ["hydraspecter", "--max-instances", "20"]
    },
    "hydraspecter-with-proxy": {
      "command": "npx",
      "args": ["hydraspecter", "--max-instances", "20", "--proxy", "http://127.0.0.1:7890"]
    },
    "hydraspecter-stealth": {
      "command": "npx",
      "args": ["hydraspecter", "--max-instances", "10", "--humanize-auto", "--channel", "chrome"]
    }
  }
} 